#include<iostream>
using namespace std;
int main()
{
    int x;
    cout<<"ingrese un numero"<<endl;
    cin>>x;
  if((x<1000)||(x>2011))
       cout<<"Ingrese un numero entre 1000-2011"<<endl;
  else
   
   {  
   		  if(x>=2000)  {cout<<"MM"   ;x=x-2000;}
   	  	if(x>=1900)  {cout<<"MCM"  ;x=x-1900;}
   		  if(x>=1800)  {cout<<"MDCCC";x=x-1800;}
   		  if(x>=1700)  {cout<<"MDCC" ;x=x-1700;}
   		  if(x>=1600)  {cout<<"MDC"  ;x=x-1600;}
   		  if(x>=1500)  {cout<<"MD"   ;x=x-1500;}
   	  	if(x>=1400)  {cout<<"MCD"  ;x=x-1400;}
   	  	if(x>=1300)  {cout<<"MCCC" ;x=x-1300;}
        if(x>=1200)  {cout<<"MCC"  ;x=x-1200;}
        if(x>=1100)  {cout<<"MC"   ;x=x-1100;}
		    if(x>=1000)  {cout<<"M"    ;x=x-1000;}
        if(x>=900)   {cout<<"CM"   ;x=x-900; }
        if(x>=500)   {cout<<"D"    ;x=x-500; }
        if(x>=400)   {cout<<"CD"   ;x=x-400; }
        if(x>=300)   {cout<<"C"    ;x=x-100; }
        if(x>=200)   {cout<<"C"    ;x=x-100; }
        if(x>=100)   {cout<<"C"    ;x=x-100; }
        if(x>=90)    {cout<<"XC"   ;x=x-90;  }
        if(x>=50)    {cout<<"L"    ;x=x-50;  }
        if(x>=40)    {cout<<"XL"   ;x=x-40;  }
        if(x>=30)    {cout<<"X"    ;x=x-10;  }
        if(x>=20)    {cout<<"X"    ;x=x-10;  }
        if(x>=10)    {cout<<"X"    ;x=x-10;  }
        if(x>=9)     {cout<<"IX"   ;x=x-9;   }
        if(x>=5)     {cout<<"V"    ;x=x-5;   }
        if(x>=4)     {cout<<"IV"   ;x=x-4;   }
        if(x>=3)     {cout<<"III"  ;x=x-3;   }
        if(x>=2)     {cout<<"II"   ;x=x-2;   }
        if(x>=1)     {cout<<"I"    ;x=x-1;   }
  
    }
   cout<<endl;
cin.ignore();return 0;
}
